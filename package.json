{
  "name": "my-blues-stack",
  "private": true,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle --external:fsevents",
    "dev": "run-p dev:*",
    "dev:server": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "remix dev --manual -c \"node --require ./mocks ./build/server.js\"",
    "docker": "docker compose up -d",
    "docker:down": "docker compose down",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run --browser chrome\"",
    "typecheck": "tsc && tsc -p cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "tf:init": "./terraform.sh init",
    "tf:plan": "./terraform.sh plan",
    "tf:apply": "./terraform.sh apply",
    "tf:destroy": "./terraform.sh destroy",
    "tf:validate": "./terraform.sh validate",
    "tf:fmt": "./terraform.sh fmt"
  },
  "prettier": {
    "printWidth": 120
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
  "dependencies": {
    "@isaacs/express-prometheus-middleware": "1.2.1",
    "@prisma/client": "4.16.2",
    "@remix-run/css-bundle": "2.3.0",
    "@remix-run/express": "2.3.0",
    "@remix-run/node": "2.3.0",
    "@remix-run/react": "2.3.0",
    "bcryptjs": "2.4.3",
    "chokidar": "3.5.3",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "express": "4.18.2",
    "express-csp-header": "5.2.1",
    "helmet": "7.1.0",
    "isbot": "3.7.1",
    "morgan": "1.10.0",
    "npm-run-all": "4.1.5",
    "prom-client": "15.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "source-map-support": "0.5.21",
    "tiny-invariant": "1.3.1"
  },
  "devDependencies": {
    "@faker-js/faker": "8.3.1",
    "@remix-run/dev": "2.3.0",
    "@testing-library/cypress": "10.0.1",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react-hooks": "8.0.1",
    "@types/bcryptjs": "2.4.6",
    "@types/compression": "1.7.5",
    "@types/eslint": "8.44.7",
    "@types/express": "4.17.21",
    "@types/morgan": "1.9.9",
    "@types/node": "18.18.6",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@types/source-map-support": "0.5.10",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "@vitejs/plugin-react": "4.2.0",
    "@vitest/coverage-v8": "0.34.6",
    "autoprefixer": "10.4.16",
    "cookie": "0.6.0",
    "cypress": "13.5.1",
    "dotenv": "16.3.1",
    "esbuild": "0.19.5",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-jest-dom": "5.1.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-markdown": "3.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "6.1.2",
    "happy-dom": "12.10.3",
    "msw": "1.3.2",
    "postcss": "8.4.31",
    "prettier": "3.1.0",
    "prettier-plugin-tailwindcss": "0.5.7",
    "prisma": "4.16.2",
    "start-server-and-test": "2.0.3",
    "tailwindcss": "3.3.5",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2",
    "vite": "4.5.0",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts"
  }
}
